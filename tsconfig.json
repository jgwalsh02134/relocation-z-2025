{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "types": ["vite/client"],
    "lib": ["ES2020", "DOM"]
  },
  "include": ["assets/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}


